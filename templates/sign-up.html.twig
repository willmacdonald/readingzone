<!doctype html>
<html lang="en" dir="ltr">
<head>
    {{ include('_head.html.twig') }}
</head>
<body>
<div id="root">
    {{ include('_header.html.twig') }}
    <main id="content">
        <div class="cols-a">
            <article>
                <header class="heading-title">
                    <h1>About Us</h1>
                    <ul class="list-social">
                        <li>Share this</li>
                        <li><a href="./"><i class="icon-facebook"></i> <span>Facebook</span></a></li>
                        <li><a href="./"><i class="icon-twitter"></i> <span>Twitter</span></a></li>
                    </ul>
                </header>
                <div class="module-box">
                    {{ data.main_text }}
                </div>
            </article>
            <aside>
                <h2 class="s35">Related Articles</h2>
                <ul class="list-related">
                    {% include ('_list-related.html.twig') %}
                </ul>
            </aside>
        </div>
    </main>
    {{ include('_footer.html.twig') }}
</div>
{{ include('_footer-scripts.html.twig') }}
</body>
</html>
